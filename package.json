{
    "name": "ors-viewer",
    "version": "1.0.0",
    "description": "",
    "main": "index.html",
    "scripts": {
        "watch:ors:dev": "webpack serve --env USE_LOCAL_STATUTES_XML --env APP_NAME=ors",
        "watch:ors:prod": "npm run watch:ors:dev --env USE_MOCK --env BASE_PATH=/abc/",
        "watch:bon:dev": "webpack serve --env USE_MOCK --env USE_LOCAL_STATUTES_XML --env APP_NAME=bon",
        "watch:bon:prod": "npm run watch:bon:dev --env USE_MOCK --env BASE_PATH=/abc/",
        "watch": "npm run watch:ors:dev --open",
        "update": "cp package.json package-main.json && package-json-merge package-main.json package-overrides.json > package.json",
        "lint": "eslint -c eslint.config.mjs ./",
        "lint:fix": "eslint -c eslint.config.mjs --fix",
        "jsdoc:build": "jsdoc -c jsdoc.json",
        "git:reset": "git reset --soft HEAD~1",
        "git:force": "git push origin +abcdefghi --force",
        "build": "webpack --config webpack.config.js --env USE_LOCAL_STATUTES_XML --env APP_NAME=ors --env BASE_PATH=/ors-viewer/",
        "serve": "serve ./dist -p 1234 -n -s",
        "serve:docs": "serve ./docs -p 1234 -n"
    },
    "dependencies": {
        "@ocdla/global-components": "file:./dev_modules/@ocdla/global-components",
        "@ocdla/lib-http": "file:./dev_modules/@ocdla/lib-http",
        "@ocdla/ors": "file:./dev_modules/@ocdla/ors",
        "@ocdla/routing": "file:./dev_modules/@ocdla/routing",
        "@ocdla/view": "^2.4.0",
        "autoprefixer": "^10.4.19",
        "globals": "^15.6.0",
        "postcss": "^8.4.39",
        "tailwindcss": "^3.4.6"
    },
    "devDependencies": {
        "@babel/cli": "^7.18.10",
        "@babel/core": "^7.19.6",
        "@babel/preset-env": "^7.19.4",
        "@babel/preset-react": "^7.24.7",
        "@eslint/js": "^9.5.0",
        "babel-loader": "^8.2.5",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.11.0",
        "eslint": "^9.7.0",
        "html-loader": "^5.1.0",
        "html-webpack-plugin": "^5.5.4",
        "jsdoc": "^4.0.3",
        "package-json-merge": "git+https://github.com/ocdladefense/package-json-merge.git",
        "postcss-loader": "^8.1.1",
        "prettier": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.5",
        "serve": "^14.2.3",
        "style-loader": "^3.3.4",
        "webpack": "^5.92.1",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.2"
    }
}
